app = angular.module('app', ['ui.bootstrap']);

app.filter 'filter', () ->
  (items,factor) ->
    response = []
    angular.forEach items, (item, key) ->
      return if factor.cpu_core isnt 'Any' and factor.cpu_core isnt item.cpu_core
      return if factor.qts4 and item.qts4 is no
      return if factor.vmware and item.vmware is no

      return unless factor.hdd_bay[item.hdd_bay]
      return if factor.hdd_sata3 and item.hdd_sata3 is no
      return if factor.hdd_ssd and item.hdd_ssd is no
      return if factor.eSATA > item.eSATA

      return if factor.lan_num > item.lan_num
      return if factor.lan_10g and item.lan_10g is no

      return if factor.usb_num > item.usb_num
      return if factor.usb_3 and item.usb_3 is no

      this.push item
    , response

    return response

MainCtrl = ($scope) ->
  $scope.items = QNAS
  $scope.factor =
    cpu_core: 'Any'
    qts4: off
    vmware: off
    hdd_bay:
     1: on
     2: on
     4: on
     5: on
     6: on
     8: on
     10: on
    hdd_sata3: off
    hdd_ssd: off
    eSATA: 0
    lan_num: 0
    lan_10g: off
    usb_num: 0
    usb_3: off

  $scope.product = {}

  angular.forEach $scope.items, (item, key) ->
    $scope.product[item.name] = item

  $scope.buy = (name) ->
    window.open('http://ecshweb.pchome.com.tw/search/v2/?q='+$scope.product[name].name)

  $scope.page = (name) ->
    window.open($scope.product[name].page)